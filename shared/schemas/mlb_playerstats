create table public.mlb_playerstats (
  id text not null,
  created_at timestamp with time zone not null default now(),
  "GAME_ID" bigint null,
  "PLAYER_ID" bigint null,
  "GAME_DATE" timestamp with time zone null,
  "SEASON_ID" integer null,
  "TEAM_ID" bigint null,
  "OPPONENT_ID" bigint null,
  "IS_HOME" boolean null,
  "STAT_TYPE" text null,
  "AB" smallint null,
  "H" smallint null,
  "R" smallint null,
  "DOUBLES" smallint null,
  "TRIPLES" smallint null,
  "HR" smallint null,
  "RBI" smallint null,
  "BB" smallint null,
  "SO" smallint null,
  "SB" smallint null,
  "CS" smallint null,
  "HBP" smallint null,
  "SF" smallint null,
  "PA" smallint null,
  "BA" real null,
  "OBP" real null,
  "SLG" real null,
  "OPS" real null,
  "IP" real null,
  "H_P" smallint null,
  "R_P" smallint null,
  "ER" smallint null,
  "BB_P" smallint null,
  "SO_P" smallint null,
  "HR_P" smallint null,
  "BF" smallint null,
  "PIT" smallint null,
  "ERA" real null,
  "WHIP" real null,
  constraint mlb_playerstats_pkey primary key (id)
) TABLESPACE pg_default;
